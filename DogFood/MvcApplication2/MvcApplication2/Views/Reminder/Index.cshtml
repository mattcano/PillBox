@model IEnumerable<MvcApplication2.Models.Reminder>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserMedicineMap.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RemindTime.RemindValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResponseTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsTaken)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SnoozeId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserMedicineMap.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RemindTime.RemindValue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResponseTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsTaken)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SnoozeId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
